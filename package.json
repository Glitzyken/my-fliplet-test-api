{
  "name": "my-fliplet-test-api",
  "version": "1.0.0",
  "description": "You've been tasked with creating an online RSS display service. This service will consist of a single web page inside a hybrid native application (with Cordova) where an end user can input an RSS URL. The URL will be validated, processed and the results will be displayed on the same page.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:prod": "set NODE_ENV=production && nodemon server.js",
    "test": "set NODE_ENV=test && mocha --recursive --exit",
    "debug": "ndb server.js"
  },
  "author": "Kenneth Jimmy",
  "license": "ISC",
